html {
    -ms-text-size-adjust: 100%;
    -webkit-text-size-adjust: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
}

input[type=button] {
    -webkit-appearance: none;
    outline: none
}

input[type=submit] {
    -webkit-appearance: none;
    outline: none
}

body {
    line-height: 1.6;
    font-family: "PingFang SC", Helvetica, Arial, "微软雅黑", "黑体"!important;
    height: 100%;
    margin: 0;
    padding: 0;
}
[v-cloak] {
  display: none;
}

* {
    margin: 0;
    padding: 0;
}

a img {
    border: 0;
}

a {
    text-decoration: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

a:link {
    text-decoration: none;
}

a:active {
    text-decoration: none;
}

a:visited {
    text-decoration: none;
}

a:hover {
    text-decoration: none!important;
}

.click-bg:active {
    background-color: #f7f7f7;
}

