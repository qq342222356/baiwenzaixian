.header .head-box {
    height: 1.12rem;
    line-height: 1.12rem;
    display: -webkit-box;
    -webkit-box-pack: justify;
    /*两端对齐*/
    -webkit-box-align: center;
}

.header .ico-wrap {
    width: 1.52rem;
    text-align: center;
}

.header .logo .ico {
    background: url(../images/logo.png)no-repeat;
    width: 0.466667rem;
    height: 0.813333rem;
    background-size: contain;
    background-position: center center;
    display: inline-block;
    vertical-align: middle;
}

.header .search-ico .ico {
    background: url(../images/search-ico.png)no-repeat;
    width: 0.453333rem;
    height: 0.453333rem;
    background-size: contain;
    background-position: center center;
    display: inline-block;
    vertical-align: middle;
}

.header .title {
    font-size: 0;
}

.header .title span {
    font-size:0.48rem;
    color: #333;
    display: inline-block;
    vertical-align: middle;
    line-height: 1.12rem;
    width: 1.5rem;
    text-align: center;
}

.header .title span.active {
    color: #d33c3e;
    position: relative;
}

.header .title span.active:after {
    content: "";
    display: block;
    position: absolute;
    left: 50%;
    margin-left:-0.6rem;
    bottom: 0;
    width:1.2rem;
    border: 0.04rem  solid #d33c3e;
    z-index: 102;
}

.header .title .dot {
    width: 6px;
    height: 6px;
    background: #2d2d2d;
    border-radius: 50%;
    vertical-align: middle;
    margin: 0 0.1rem;
    display: inline-block;
}
